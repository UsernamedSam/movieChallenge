{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "import/extensions": 0,
      "no-var": "error",
      "prefer-const": "error",
      "eqeqeq": "error",
      "indent": ["error", 2]
    }
  }
 